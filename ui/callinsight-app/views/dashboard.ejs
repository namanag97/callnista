<%- include('partials/layout', { title: 'Dashboard', body: `
<div class="page-header">
    <div class="page-title">
        <h1>Dashboard</h1>
        <div class="page-subtitle">Overview of your call analytics system</div>
    </div>
    
    <div class="action-buttons">
        <button class="btn btn-secondary">
            <svg data-feather="sliders"></svg>
            <span>Filters</span>
        </button>
        <button class="btn btn-primary" id="uploadBtn">
            <svg data-feather="upload"></svg>
            <span>Upload Calls</span>
        </button>
    </div>
</div>

<div class="actions-bar">
    <div class="search-bar">
        <svg data-feather="search"></svg>
        <input type="text" placeholder="Search by call ID, agent, or issue...">
    </div>
    
    <div class="action-buttons">
        <button class="btn btn-secondary">
            <svg data-feather="calendar"></svg>
            <span>Last 7 days</span>
        </button>
        <button class="btn btn-secondary">
            <svg data-feather="download"></svg>
            <span>Export</span>
        </button>
    </div>
</div>

<!-- Dashboard Stats -->
<div class="dashboard-grid">
    <div class="stat-card">
        <div class="stat-card-title">Calls Processed</div>
        <div class="stat-card-value">1,247</div>
        <div class="stat-card-footer">
            <span class="stat-card-badge badge-up">+7.2%</span>
            <span>vs last week</span>
        </div>
    </div>
    
    <div class="stat-card">
        <div class="stat-card-title">Avg. Processing Time</div>
        <div class="stat-card-value">3.4 min</div>
        <div class="stat-card-footer">
            <span class="stat-card-badge badge-up">+1.5%</span>
            <span>vs last week</span>
        </div>
    </div>
    
    <div class="stat-card">
        <div class="stat-card-title">Resolution Rate</div>
        <div class="stat-card-value">82.5%</div>
        <div class="stat-card-footer">
            <span class="stat-card-badge badge-up">+3.8%</span>
            <span>vs last week</span>
        </div>
    </div>
    
    <div class="stat-card">
        <div class="stat-card-title">Critical Issues</div>
        <div class="stat-card-value">17</div>
        <div class="stat-card-footer">
            <span class="stat-card-badge badge-down">-12.3%</span>
            <span>vs last week</span>
        </div>
    </div>
</div>

<!-- Recent Call Analysis Table -->
<div class="content-card">
    <div class="content-card-header">
        <div class="content-card-title">Recent Call Analysis</div>
        <div>
            <select class="form-control">
                <option>All Categories</option>
                <option>Account Issues</option>
                <option>Transaction Problems</option>
                <option>Technical Support</option>
                <option>Product Inquiries</option>
            </select>
        </div>
    </div>
    
    <div class="content-card-body">
        <div class="table-responsive">
            <table>
                <thead>
                    <tr>
                        <th>Call ID</th>
                        <th>Date & Time</th>
                        <th>Duration</th>
                        <th>Category</th>
                        <th>Severity</th>
                        <th>Status</th>
                        <th>Agent</th>
                        <th>Actions</th>
                    </tr>
                </thead>
                <tbody>
                    <!-- Sample data rows -->
                    <tr>
                        <td>CALL-2023-0452</td>
                        <td>Aug 21, 2023 14:35</td>
                        <td>08:42</td>
                        <td>Account Access > Login Issue</td>
                        <td><span class="badge badge-warning">Medium</span></td>
                        <td><span class="badge badge-success">Resolved</span></td>
                        <td>Raj Sharma</td>
                        <td>
                            <button class="btn btn-secondary" style="padding: 0.25rem 0.5rem; font-size: 0.75rem;">
                                <svg data-feather="eye" width="14"></svg>
                                <span>View</span>
                            </button>
                        </td>
                    </tr>
                    <tr>
                        <td>CALL-2023-0451</td>
                        <td>Aug 21, 2023 13:22</td>
                        <td>15:18</td>
                        <td>Transactions > Failed Transfer</td>
                        <td><span class="badge badge-danger">Critical</span></td>
                        <td><span class="badge badge-info">Escalated</span></td>
                        <td>Priya Patel</td>
                        <td>
                            <button class="btn btn-secondary" style="padding: 0.25rem 0.5rem; font-size: 0.75rem;">
                                <svg data-feather="eye" width="14"></svg>
                                <span>View</span>
                            </button>
                        </td>
                    </tr>
                </tbody>
            </table>
        </div>
        
        <div class="table-pagination">
            <div class="table-info">
                Showing 1 to 2 of 128 entries
            </div>
            <div class="pagination-controls">
                <button class="pagination-button">Previous</button>
                <button class="pagination-button active">1</button>
                <button class="pagination-button">2</button>
                <button class="pagination-button">3</button>
                <button class="pagination-button">Next</button>
            </div>
        </div>
    </div>
</div>

<!-- Upload Modal -->
<div class="modal-overlay" style="display: none;" id="uploadModal">
    <div class="modal">
        <div class="modal-header">
            <div class="modal-title">Upload Call Recordings</div>
            <button class="modal-close" id="closeModal">
                <svg data-feather="x"></svg>
            </button>
        </div>
        <div class="modal-body">
            <form action="/upload" method="post" enctype="multipart/form-data">
                <div class="upload-area" id="dropArea">
                    <div class="upload-icon">
                        <svg data-feather="upload-cloud" width="48" height="48"></svg>
                    </div>
                    <div class="upload-text">
                        <strong>Click to upload</strong> or drag and drop
                    </div>
                    <div class="upload-hint">
                        Supports AAC audio files (Max: 50MB)
                    </div>
                    <input type="file" id="fileInput" name="audioFiles" style="display: none;" multiple accept=".aac">
                </div>
                
                <div class="form-group">
                    <label class="form-label">Processing Queue</label>
                    <select class="form-control" name="processingQueue">
                        <option value="standard">Standard Queue (Processing time: ~30 mins)</option>
                        <option value="priority">Priority Queue (Processing time: ~10 mins)</option>
                        <option value="batch">Batch Processing (Schedule for off-peak hours)</option>
                    </select>
                </div>
                
                <div class="form-group">
                    <label class="form-label">Tags (Optional)</label>
                    <input type="text" class="form-control" name="tags" placeholder="E.g., product-inquiry, escalation, hindi">
                </div>
                
                <div class="modal-footer">
                    <button type="button" class=